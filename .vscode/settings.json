{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "prettier.trailingComma": "es5",
  "prettier.arrowParens": "always",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/dist": true,
    "**/node_modules": true
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#70a3cc",
    "activityBar.background": "#70a3cc",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#a43b75",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#70a3cc",
    "statusBar.background": "#4b8bbe",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#70a3cc",
    "statusBarItem.remoteBackground": "#4b8bbe",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#4b8bbe",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#4b8bbe99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#4B8BBE"
}